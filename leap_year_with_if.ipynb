   ],
   "source": [
    "\n",
    "x = int(input('Bir yil giriniz: '))\n",
    "\n",
    "def ly(year):\n",
    "    if (( year % 4 == 0) and ( year % 100 != 0)) or \\\n",
    "     ( year % 400 == 0 ):\n",
    "        print(f\"{year} is a leap year\")\n",
    "    else:\n",
    "        print(f\"{year} is a not leap year\")\n",
    "        \n",
    "ly(x)"
